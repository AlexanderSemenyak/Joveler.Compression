<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net451;netstandard1.3;netstandard2.0</TargetFrameworks>
    <TargetFrameworks Condition="'$(OS)' != 'Windows_NT'">netstandard1.3;netstandard2.0</TargetFrameworks>
    <Title>Joveler.Compression.ZLib</Title>
    <Version>2.1.0</Version>
    <Copyright>Copyright (c) 2017-2018 Hajin Jang</Copyright>
    <Description>C# pinvoke library for zlib.
Provides improved DeflateStream, ZLibStream, GZipStream and Adler32Stream, Crc32Stream.
Supports Windows and Linux.</Description>
    <Authors>Jean-loup Gailly, Mark Adler, Hajin Jang</Authors>
    <PackageLicenseUrl>https://github.com/ied206/Joveler.Compression/blob/master/Joveler.Compression.ZLib/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/ied206/Joveler.Compression</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ied206/Joveler.Compression</RepositoryUrl>
    <PackageTags>zlib deflate gzip adler32 crc32 compression pinvoke interop</PackageTags>
    <PackageReleaseNotes>- Support .Net Framework 4.5.1 and .Net Standard 1.3</PackageReleaseNotes>
    <AssemblyVersion>2.1.0.0</AssemblyVersion>
    <FileVersion>2.1.0.0</FileVersion>
    <LangVersion>latest</LangVersion>
    <PackageId />
    <Company>Joveler</Company>
    <PackageIconUrl>https://raw.githubusercontent.com/ied206/Joveler.Compression/master/Image/Logo.png</PackageIconUrl>
  </PropertyGroup>
  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>
  <Import Project="$(MSBuildProjectDirectory)\Joveler.Compression.ZLib.targets" />
</Project>
  