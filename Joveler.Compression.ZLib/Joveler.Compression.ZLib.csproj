<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net451;netstandard2.0</TargetFrameworks>
    <TargetFrameworks Condition="'$(OS)' != 'Windows_NT'">netstandard2.0</TargetFrameworks>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Title>Joveler.Compression.ZLib</Title>
    <Version>2.2.0</Version>
    <Copyright>Copyright (c) 2017-2019 Hajin Jang</Copyright>
    <Description>C# pinvoke library for zlib.
Provides improved DeflateStream, ZLibStream, GZipStream and Adler32Stream, Crc32Stream.
Supports Windows and Linux.</Description>
    <Authors>Jean-loup Gailly, Mark Adler, Hajin Jang</Authors>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/ied206/Joveler.Compression</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ied206/Joveler.Compression</RepositoryUrl>
    <PackageTags>zlib deflate gzip adler32 crc32 compress compression pinvoke interop</PackageTags>
    <PackageReleaseNotes>- Include native armhf, arm64 linux binary</PackageReleaseNotes>
    <LangVersion>7.3</LangVersion>
    <PackageId />
    <Company>Joveler</Company>
    <PackageIconUrl>https://raw.githubusercontent.com/ied206/Joveler.Compression/master/Image/Logo.png</PackageIconUrl>
    <PackageLicenseExpression>Zlib</PackageLicenseExpression>
  </PropertyGroup>
  <Import Project="$(MSBuildProjectDirectory)\Joveler.Compression.ZLib.targets" />
  <ItemGroup>
    <PackageReference Include="System.Memory" Version="4.5.2" />
  </ItemGroup>
</Project>
  